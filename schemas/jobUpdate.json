{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "http://example.com/example.json",
	"type": "object",
	"default": {},
	"title": "Root Schema",
	"required": ["title"],
	"properties": {
		"title": {
			"type": "string",
			"default": "",
			"title": "The title Schema",
			"examples": ["software engineer"]
		},
		"salary": {
			"type": "integer",
			"default": 0,
			"title": "The salary Schema",
			"examples": [0]
		},
		"equity": {
			"type": "string",
			"default": "",
			"title": "The equity Schema",
			"examples": ["string"]
		}
	},
	"examples": [
		{
			"title": "software engineer",
			"salary": 0,
			"equity": "string"
		}
	]
}
